/* pages/add/add.wxss */
.add{
  margin:75rpx;
  font-family: 黑体;
}
.addBlock{
  margin:75rpx auto;
}
.addTitle{
  color: rgba(128, 128, 128, 0.486);
}
.addTitleInput{
  color: rgb(0, 0, 0);
}
.line{
margin: 20rpx auto 30rpx auto;
height: 2rpx;
background-color: #D8D8D8;
}
.imageAndPlus{
  display: flex;
  flex-wrap: wrap;
}
.plusButton{
  width:147rpx;
  height:147rpx;
  text-align: center;
  border:3rpx solid rgba(128, 128, 128, 0.486);
  color: rgba(128, 128, 128, 0.486);
  margin:10rpx 10rpx;
}
.plusButtonText{
  line-height: 122.5rpx;
  font-weight: 100;
  font-size: 150rpx;
}
.showImage{
  width:150rpx;
  height:150rpx;
  background-color: black;
  margin:10rpx 10rpx;
}
.record{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.recordButton{
  width:150rpx;
  height:150rpx;
  margin:10rpx;
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-color: white;
  background-position: center center;
  border-radius: 50%;
}
.stopRecord{
  width:100rpx;
  height:100rpx;
  background-color: red;
  border-radius: 50%;
  margin:20rpx auto;
}
.recordOperation{
  display: flex;
}
.replayButton{
